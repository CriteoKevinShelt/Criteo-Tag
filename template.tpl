//Inserts Criteo's script
require('injectScript')('https://static.criteo.net/js/ld/ld.js', data.gtmOnSuccess, data.gtmOnFailure);
//START - Global variables used for accessing dataLayer, window, etc
const log = require('logToConsole'); 
const getFromWindow = require('copyFromWindow');
const dataLayer = getFromWindow('dataLayer');
const getFromDataLayer = require('copyFromDataLayer');

//Debugging in console
log("START CRITEO DEBUGGING");

//Error message storage
let eMessages = ["Could not find hashed user email. Cross-Device capabilities will be compromised.","None found","No relevant dataLayer found","No products found in product page dataLayer","No products found in listing/category dataLayer","No products found in basket dataLayer","No products found in sales dataLayer","Transaction ID not found"];

//Assignment of global dataLayer variable product paths and triggers
//h = home, l = listing, p = product, b = basket, s = sales
let hEvent, lEvent, pEvent, bEvent, sEvent;
let hKey, lKey, pKey, bKey, sKey;
//lp = listing page, pp = product page, bp = basket page, sp = sales page
let lpProducts, ppProducts, bpProducts, spProducts;
//TransactionIdPath
let tId;
//DataLayer being fetched from template UI
const dlType = data.dlType;

const criteo = "criteo";
const enhanced = "enhanced";
const custom = "custom";
const event = "event";

//Transaction ID path
tId = (dlType === criteo ? "crto.transactionid" : (dlType === enhanced ? "ecommerce.purchase.actionField.id" : data.SPTP));

//Aggregated pages - key-value store
let pageData = [
  {name: "home", key: getKeys(data.HPDEN, "HPK"), value: getValues(data.HPDEN, "HPV", "home")},
  {name: "listing", key: getKeys(data.LPDEN, "LPK"), value: getValues(data.LPDEN, "LPV", "listing"), products: getProductPaths("listing")},
  {name: "product", key: getKeys(data.PPDEN, "PPK"), value: getValues(data.PPDEN, "PPV", "product"), products: getProductPaths("product")},
  {name: "basket", key: getKeys(data.BPDEN, "BPK"), value: getValues(data.BPDEN, "BPV", "basket"), products: getProductPaths("basket")},
  {name: "transaction", key: getKeys(data.SPDEN, "SPK"), value: getValues(data.SPDEN, "SPV", "sales"), products: getProductPaths("sales")}
];

function getKeys(table, keyName){
  let keys = [];
  if(!table){return keys;}
  if(dlType === custom){
    for(let i=0; i<table.length; i++){
      keys.push(table[i][keyName]+"");
    }
  }else{
    keys.push(event);
  }
  return keys;
}

function getValues(table, valueName, page){
  let values = [];
  if(!table){return values;}
  let isCriteo = (dlType===criteo);
  if(dlType === custom){
    for(let i=0; i<table.length; i++){
      values.push(table[i][valueName]);
    }
  }else{
    switch(page){
      case "home":
      	values.push(isCriteo ? "crto_homepage" : "not_tracked");
        break;
      case "listing":
        values.push(isCriteo ? "crto_listingpage" : "productImpression");
        break;
      case "product":
        values.push(isCriteo ? "crto_productpage" : "productDetail");
        break;
      case "basket":
        if(isCriteo){
          values.push("crto_basketpage");
        }else{
          values.push("checkout");
          values.push("addToCart");
          values.push("removeFromCart");
        }
        break;
      case "sales":
        values.push(isCriteo ? "crto_transactionpage" : "purchase");
        break;
    }
  }
  return values;
}

function getProductPaths(page){
  switch(page){
    case "listing":
      return (dlType === criteo ? "crto.products" : (dlType === enhanced ? "ecommerce.impressions" : data.LPP));
    case "product":
      return (dlType === criteo ? "crto.products" : (dlType === enhanced ? "ecommerce.detail.products" : data.PPP));
    case "basket":
      return (dlType === criteo ? "crto.products" : (dlType === enhanced ? "ecommerce.checkout.products" : data.BPP));
    case "sales":
      return (dlType === criteo ? "crto.products" : (dlType === enhanced ? "ecommerce.purchase.products" : data.SPP));
  }
}

//fetch and store globally the index of Criteo or e-commerce dataLayer object
let pageType;
let pageIndex;
let dlIndex = cdlEventIndex();
let dlObject = dataLayer[dlIndex];

//END - Determine dataLayer variables being used in tags
//END - Global Library of dataLayer objects.
function getUserEmail(){
  if(dlObject){
    if(dlType==="criteo" && dlObject.crto.email !== ""){
      return dlObject.crto.email;
    }
    else if(dlType==="custom" && dlObject.email !== ""){
      return dlObject.email;
    }else{
      printer(false,"getUserEmail",pageType,0,false);
      return "";
    }
  }
}
//Fetches site type from GTM variable
function getSiteType(){
  return data.siteType;
}
//Page-specific 
function getPageData(){
  if(dlIndex<0){
    printer(false,"getPageData",pageType,2,false);
    return {event: "viewHome", tms: "gtm-criteo-2.0.0" };
  }
  let pageName = findPageType();
  let ctms = "gtm-criteo-2.0.0";
  switch(pageName){
    case "home":
      return {event: "viewHome", tms: ctms };
    case "listing":
      return {event: "viewList", tms: ctms, item: getListingData()};
    case "product":
      return {event: "viewItem", tms: ctms,  item: getProductPageData()};
    case "basket":
      return {event: "viewBasket", tms: ctms, item: getBasketData()};
    case "transaction":
      return {event: "trackTransaction", tms: ctms, id: getTransactionId(), item: getSalesData()};
  }
}
//Find the index of the necessary dataLayer object
function cdlEventIndex(){
  for(let h=dataLayer.length-1; h>=0; h--){
    let dl = dataLayer[h];
    for(let i=0; i<pageData.length; i++){
      let keySet = pageData[i].key;
      for(let j=0; j<keySet.length; j++){
        let currentKey = parseDlData(keySet[j],dl);
        if(currentKey){
          let valueSet = pageData[i].value;
          if(valueSet.indexOf(currentKey) != -1){
            pageIndex = i;
            pageType = pageData[i].name;
            return h;
          }
        }
      }
    }
  }
  return -1;
}
function getProducts(){
  for(let h=dataLayer.length-1; h>=0; h--){
    let dl = dataLayer[h];
    let products = parseDlData(pageData[pageIndex].products, dl);
    if(products){
      return products;
    }
  }
  return "no_products";
}
//Find page type [event, Event, PageType, pageType, etc] in initial stages
function findPageType(){
  if(pageType){
    return pageType;
  }else
  	return "home";
}

function checkAllUrls(urlToCheck){
  return false;
}
//Fetches the product ID from the product page
function getProductPageData(){
  let id = getProducts();
  if(require('getType')(id)==="array"){
    if(id[0].hasOwnProperty("id")){
      id = id[0].id;
    }else if(id[0].hasOwnProperty("sku")){
      id = id[0].sku;
    }
  }
  if(!id){
    printer(false,"getProductPagedata",pageType,3,false);
	return "NO_ID_FOUND";
  }else{
    printer(true,false,false,false,id);
    return id;
  }
}
//Gets at most three unique parent product Ids from the listing/category page
function getListingData(){
  let ids = [];
  let listOfItems = getProducts();
  log(listOfItems);
  if(dlType==="criteo" || (dlType==="custom" && !listOfItems[0].id)){
    printer(true,false,false,false,listOfItems);
    return listOfItems;
  }
  for(let i=0; ids.length<3 && i<listOfItems.length; i++){
    let id = listOfItems[i].id.split("-")[0];
    if(ids.indexOf(id) == -1){
	  printer(true,false,false,false,id);
      ids.push(id);
    }
  }
  if(ids.length<1){
	printer(false,"getListingData",pageType,3,false);
  }
  return ids;
}
//Gets the relevant product data points from basket page
function getBasketData(){
  let products = []; 
  let cartItems = getProducts();    
  cartItems.forEach(function(item){ 
	products.push({id: item.id, price: item.price, quantity: item.quantity});
	printer(true,false,false,false,item.id);
  });
  if(products.length<1){
	printer(false,"getBasketData",pageType,4,false);
  }
  return products;
}
//Gets the relevant product data points from sales page
function getSalesData(){
  let products = [];
  let cartItems = getProducts();
  cartItems.forEach(function(item){ 
	products.push({id: item.id, price: item.price, quantity: item.quantity});
	printer(true,false,false,false,item.id);
  });
  if(products.length<1){
	printer(false,"getSalesData",pageType,5,false);
  }
  return products;
}
//Gets the transaction ID if one exists or generates a transaction Id if none exist
function getTransactionId(){
  tId = parseDlData(tId, dlObject);
  if(tId){
    return tId;
  }else{
    printer(false,"getTransactionId",pageType,6,false);
    return "Transaction_ID_Not_Found";
  }
}
//Debugging messages (only displayed in preview mode)
function printer(successful, functionMessage, dataLayerMessage, mIndex, product){
  //success message
  if(successful){
    log("SUCCESS - Product Added: " + product + ".");
  }else{
    log("FAIL - Function Affected: " + functionMessage + ". " + "DataLayer object affected: " + dataLayerMessage + ". Error Message: " + eMessages[mIndex]);
  }
  //fail message
}
//Parses variable paths provided by user (e.g. ecommerce.purchase.products)
function parseDlData(cData, dl){
  let tokens = cData.split(".");
  let path = dl;
  for(let i=0; i<tokens.length; i++){
    if(path.hasOwnProperty(tokens[i])){
      path = path[tokens[i]];
  	}else{
      return false;
    }
  }
  return path;
}
const criteoQPush = require('createQueue')('criteo_q');
//Common data points
criteoQPush(
  { event: "setAccount", account: data.accountID },
  { event: "setEmail", email: getUserEmail() },
  { event: "setSiteType", type: getSiteType() },
  getPageData()
);
log("END CRITEO DEBUGGING");
// Call data.gtmOnSuccess when the tag is finished.


